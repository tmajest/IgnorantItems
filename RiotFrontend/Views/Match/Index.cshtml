@section scripts {
    @Scripts.Render("~/Scripts/Apps/matchController.js")
}
<script type='text/javascript'>
    window.backendParams = { matchId: @ViewBag.MatchId };
</script>

<div ng-cloak id="matchContainer" ng-app="IgnorantItems" ng-controller="matchController">
    <div id="matchInfoHeader" class="row">
        <div class="matchChampionImage">
            <div class="col-xs-2">
                <img ng-src="http://ddragon.leagueoflegends.com/cdn/5.24.2/img/champion/{{imageName}}" width="60" height="60" />
            </div>
            <div class="col-xs-6">
                <h2>{{title}}</h2>
            </div>
        </div>
    </div>
    <hr />
    <div class="matchInfoContainer">
        <h3>Match Info</h3>
        <div class="row">
            <div class="col-xs-2">Score:</div>
            <div class="col-xs-2">
                <span class="kills">{{kills}}</span> /
                <span class="deaths">{{deaths}}</span> /
                <span class="assists">{{assists}}</span>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-2">Champion:</div>
            <div class="col-xs-2">{{match.Champion.Name}}</div>
        </div>
        <div class="row">
            <div class="col-xs-2">Duration:</div>
            <div class="col-xs-2">{{durationText}}</div>
        </div>
    </div>

    <hr />
    <div class="summonerSpellsContainer">
        <h3>Summoner Spells</h3>
        <div class="summonerSpell">
            <div class="row">
                <img ng-src="http://ddragon.leagueoflegends.com/cdn/5.24.2/img/spell/{{summonerSpells[spell1Id].Image.Full}}" popover-placement="top" popover-title="{{summonerSpells[spell1Id].Name}}" uib-popover="{{summonerSpells[spell1Id].Description}}" popover-trigger="mouseenter" />
            </div>
        </div>
        <div class="summonerSpell">
            <div class="row">
                <img ng-src="http://ddragon.leagueoflegends.com/cdn/5.24.2/img/spell/{{summonerSpells[spell2Id].Image.Full}}" popover-placement="top" popover-title="{{summonerSpells[spell2Id].Name}}" uib-popover="{{summonerSpells[spell2Id].Description}}" popover-trigger="mouseenter" />
            </div>
        </div>
    </div>

    <hr />
    <div class="runesContainer">
        <h3>Runes</h3>
        <div class="row" ng-repeat="rune in match.Runes">
            <div class="runeContainer" > 
                <div class="runeRow">
                    <div class="col-xs-2">
                        <img ng-src="http://ddragon.leagueoflegends.com/cdn/5.24.2/img/rune/{{rune.Data.Image.Full}}" width="35" height="35" popover-placement="top" popover-title="{{rune.Data.Name}}" uib-popover="{{rune.Data.SanitizedDescription}}" popover-trigger="mouseenter" />
                        <span>x{{rune.Rank}}</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="runeDescription">
                            <span>{{rune.Data.SanitizedDescription}}</span>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>

